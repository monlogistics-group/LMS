<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="lms_operation_type_datas_action" model="ir.actions.act_window">
		<field name="name">Operation type</field>
		<field name="res_model">lms.operation.type.datas</field>
		<field name="view_mode">tree,form</field>
	</record>
	<record id="lms_operation_type_datas_form" model="ir.ui.view">
		<field name="name">lms.datas.operation.form</field>
		<field name="model">lms.operation.type.datas</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="operation_id"/>
						<field name="name" />
						<field name="description" />
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<record id="lms_operation_type_datas_tree" model="ir.ui.view">
		<field name="name">lms.datas.operation.tree</field>
		<field name="model">lms.operation.type.datas</field>
		<field name="arch" type="xml">
			<tree>
				<field name="operation_id" />
				<field name="name" />
				<field name="description" />
			</tree>
		</field>
	</record>
</odoo>