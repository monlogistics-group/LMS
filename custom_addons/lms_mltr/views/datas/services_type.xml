<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="lms_operation_service_type_action" model="ir.actions.act_window">
		<field name="name">Services type</field>
		<field name="res_model">lms.operation.service.types</field>
		<field name="view_mode">tree,form</field>
	</record>
	<record id="lms_operation_service_type_form" model="ir.ui.view">
		<field name="name">lms.datas.service.type.form</field>
		<field name="model">lms.operation.service.types</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field name="active"/>
						<field name="name" />
						<field name="description" />
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<record id="lms_operation_service_type_tree" model="ir.ui.view">
		<field name="name">lms.datas.service.type.tree</field>
		<field name="model">lms.operation.service.types</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
				<field name="description" />
			</tree>
		</field>
	</record>
</odoo>