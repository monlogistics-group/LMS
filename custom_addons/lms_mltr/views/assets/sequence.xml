<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<!-- 
		Sequence modification
	-->
	<data>
		<!-- 
			name: General sequence
			model: lms.general
		-->
		<record id="seq_lms_general" model="ir.sequence">
			<field name="name">LMS General Sequence</field>
			<field name="code">lms.general</field>
			<field name="prefix">G</field>
			<field name="padding">8</field>
			<field name="company_id" eval="False" />
		</record>
		<!-- 
			name: Operation Type Sequence
			model: lms.operation
		-->
		<record id="seq_lms_operation" model="ir.sequence">
			<field name="name">LMS Operation Type Sequence</field>
			<field name="code">lms.operation</field>
			<field name="padding">2</field>
			<field name="company_id" eval="False" />
		</record>
		<!-- 
			name: General Order Sequence<
			model: lms.general.order
		-->
		<record id="seq_lms_operation_order" model="ir.sequence">
			<field name="name">LMS General Order Sequence</field>
			<field name="code">lms.general.order</field>
			<field name="prefix">O</field>
			<field name="padding">8</field>
			<field name="company_id" eval="False" />
		</record>
		<!-- 
			name: General Package Sequence
			model: lms.general.package
		-->
		<record id="seq_lms_operation_order" model="ir.sequence">
			<field name="name">LMS General Package Sequence</field>
			<field name="code">lms.general.package</field>
			<field name="prefix">P</field>
			<field name="padding">8</field>
			<field name="company_id" eval="False" />
		</record>
	</data>
</odoo>